{
  "name": "@crocswap-libs/ambient-utils",
  "version": "0.1.0",
  "license": "MIT",
  "author": "surudhb",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "npx dts watch",
    "test": "dts test",
    "postinstall": "npx husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "dts lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@size-limit/preset-small-lib": "^10.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "size-limit": "^10.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@covalenthq/client-sdk": "^0.2.6",
    "@crocswap-libs/sdk": "^0.2.79",
    "d3": "^7.4.4",
    "ethers": "^5.6.5",
    "modern-screenshot": "^4.4.25",
    "numbro": "^2.3.6"
  }
}
